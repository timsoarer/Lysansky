  dialogue:
  - speaker: "\u0411\u043E\u0446\u043C\u0430\u043D"
    speech: "\u0414\u043E\u0431\u0440\u043E \u043F\u043E\u0436\u0430\u043B\u043E\u0432\u0430\u0442\u044C,
      \u041B\u0438\u0441\u044F\u043D\u0441\u043A\u0438\u0439! \u0420\u0430\u0434
      \u0432\u0438\u0434\u0435\u0442\u044C \u0442\u0435\u0431\u044F. \u041D\u0430\u0441\u043A\u043E\u043B\u044C\u043A\u043E
      \u0443\u043A\u043E\u043C\u043F\u043B\u0435\u043A\u0442\u043E\u0432\u0430\u043D
      \u043A\u043E\u0440\u0430\u0431\u043B\u044C \u043F\u0435\u0440\u0435\u0434 \u043E\u0442\u043F\u043B\u044B\u0442\u0438\u0435\u043C?"
  - speaker: "\u041B\u0438\u0441\u044F\u043D\u0441\u043A\u0438\u0439"
    speech: "\u0417\u0434\u0440\u0430\u0432\u0441\u0442\u0432\u0443\u0439, \u0431\u043E\u0446\u043C\u0430\u043D!
      \u0412\u0441\u0435 \u0433\u043E\u0442\u043E\u0432\u043E \u043A \u043E\u0442\u043F\u043B\u044B\u0442\u0438\u044E,
      \u043D\u043E \u043F\u0435\u0440\u0435\u0434 \u0442\u0435\u043C, \u043A\u0430\u043A
      \u043C\u044B \u043F\u043E\u0434\u043D\u0438\u043C\u0435\u043C \u044F\u043A\u043E\u0440\u044C,
      \u043D\u0443\u0436\u043D\u043E \u043F\u0440\u043E\u0432\u0435\u0441\u0442\u0438
      \u0438\u043D\u0432\u0435\u043D\u0442\u0430\u0440\u0438\u0437\u0430\u0446\u0438\u044E
      \u043F\u0440\u0438\u043F\u0430\u0441\u043E\u0432 \u0438 \u0441\u043D\u0430\u0440\u044F\u0436\u0435\u043D\u0438\u044F.
      \u041C\u043E\u0433\u0443 \u043B\u0438 \u044F \u043F\u0440\u0438\u0441\u0442\u0443\u043F\u0438\u0442\u044C
      \u043A \u044D\u0442\u043E\u0439 \u0437\u0430\u0434\u0430\u0447\u0435?"
  - speaker: "\u0411\u043E\u0446\u043C\u0430\u043D"
    speech: "\u041A\u043E\u043D\u0435\u0447\u043D\u043E, \u041B\u0438\u0441\u044F\u043D\u0441\u043A\u0438\u0439.
      \u041F\u0440\u043E\u0432\u0435\u0440\u044C \u043A\u043E\u043B\u0438\u0447\u0435\u0441\u0442\u0432\u043E
      \u0435\u0434\u044B, \u0432\u043E\u0434\u044B, \u043C\u0435\u0434\u0438\u0446\u0438\u043D\u0441\u043A\u0438\u0445
      \u043F\u0440\u0438\u043F\u0430\u0441\u043E\u0432 \u0438 \u0441\u043E\u043E\u0431\u0449\u0438
      \u043C\u043D\u0435 \u043E \u0440\u0435\u0437\u0443\u043B\u044C\u0442\u0430\u0442\u0430\u0445.
      \u041F\u0440\u0438 \u043D\u0435\u043E\u0431\u0445\u043E\u0434\u0438\u043C\u043E\u0441\u0442\u0438
      \u0434\u043E\u043F\u043E\u043B\u043D\u0438 \u0437\u0430\u043F\u0430\u0441\u044B
      \u0443 \u043F\u043E\u0441\u0442\u0430\u0432\u0449\u0438\u043A\u043E\u0432 \u0432
      \u043F\u043E\u0440\u0442\u0443."
