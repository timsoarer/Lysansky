  dialogue:
  - speaker: "\u041B\u0438\u0441\u044F\u043D\u0441\u043A\u0438\u0439"
    speech: "\u0412\u0430\u0448\u0435 \u0412\u0435\u043B\u0438\u0447\u0435\u0441\u0442\u0432\u043E,
      \u0412\u044B \u0432\u044B\u0437\u044B\u0432\u0430\u043B\u0438 \u043C\u0435\u043D\u044F?"
  - speaker: "\u041F\u0435\u0442\u0440 \u041F\u0435\u0440\u0432\u044B\u0439"
    speech: "\u041F\u043E\u0434\u043D\u0438\u043C\u0438\u0441\u044C, \u044E\u043D\u043E\u0448\u0430.
      \u042F \u0441\u043B\u044B\u0448\u0430\u043B \u043E \u0442\u0432\u043E\u0438\u0445
      \u0443\u0441\u043F\u0435\u0445\u0430\u0445 \u0432 \u043C\u043E\u0440\u0441\u043A\u043E\u0439
      \u0430\u043A\u0430\u0434\u0435\u043C\u0438\u0438.\n\u0422\u044B \u043C\u043D\u043E\u0433\u043E
      \u0447\u0435\u043C\u0443 \u043D\u0430\u0443\u0447\u0438\u043B\u0441\u044F,
      \u043D\u043E \u043D\u0430\u0441\u0442\u043E\u044F\u0449\u0435\u0435 \u043E\u0431\u0443\u0447\u0435\u043D\u0438\u0435
      \u043D\u0430\u0447\u0438\u043D\u0430\u0435\u0442\u0441\u044F \u0432 \u043E\u0442\u043A\u0440\u044B\u0442\u043E\u043C
      \u043C\u043E\u0440\u0435."
  - speaker: "\u041B\u0438\u0441\u044F\u043D\u0441\u043A\u0438\u0439"
    speech: "\u042F \u0433\u043E\u0442\u043E\u0432 \u0441\u043B\u0435\u0434\u043E\u0432\u0430\u0442\u044C
      \u0437\u0430 \u0412\u0430\u0448\u0438\u043C\u0438 \u0443\u043A\u0430\u0437\u0430\u043D\u0438\u044F\u043C\u0438
      \u0438 \u0441\u043B\u0443\u0436\u0438\u0442\u044C \u0420\u043E\u0441\u0441\u0438\u0439\u0441\u043A\u043E\u043C\u0443
      \u0444\u043B\u043E\u0442\u0443, \u0412\u0430\u0448\u0435 \u0412\u0435\u043B\u0438\u0447\u0435\u0441\u0442\u0432\u043E."
  - speaker: "\u041F\u0435\u0442\u0440 \u041F\u0435\u0440\u0432\u044B\u0439"
    speech: "\u041C\u044B \u0441\u0442\u043E\u0438\u043C \u043D\u0430 \u043F\u043E\u0440\u043E\u0433\u0435
      \u0432\u0435\u043B\u0438\u043A\u0438\u0445 \u0438\u0437\u043C\u0435\u043D\u0435\u043D\u0438\u0439.
      \u041C\u043E\u0440\u0435 - \u044D\u0442\u043E \u043F\u0443\u0442\u044C \u043A
      \u043D\u043E\u0432\u044B\u043C \u0437\u043D\u0430\u043D\u0438\u044F\u043C \u0438
      \u0431\u043E\u0433\u0430\u0442\u0441\u0442\u0432\u0430\u043C. \u0422\u044B
      \u0433\u043E\u0442\u043E\u0432 \u043F\u0440\u0438\u043D\u044F\u0442\u044C \u0432\u044B\u0437\u043E\u0432
      \u0438 \u0441\u0442\u0430\u0442\u044C \u0447\u0430\u0441\u0442\u044C\u044E
      \u043A\u0440\u0443\u0433\u043E\u0441\u0432\u0435\u0442\u043D\u043E\u0439 \u044D\u043A\u0441\u043F\u0435\u0434\u0438\u0446\u0438\u0438,
      \u0440\u0430\u0441\u0448\u0438\u0440\u044F\u044F \u0433\u0440\u0430\u043D\u0438\u0446\u044B
      \u043D\u0430\u0448\u0435\u0439 \u0438\u043C\u043F\u0435\u0440\u0438\u0438?"
  - speaker: "\u041B\u0438\u0441\u044F\u043D\u0441\u043A\u0438\u0439"
    speech: "\u0421 \u0447\u0435\u0441\u0442\u044C\u044E, \u0412\u0430\u0448\u0435
      \u0412\u0435\u043B\u0438\u0447\u0435\u0441\u0442\u0432\u043E. \u042F \u0433\u043E\u0442\u043E\u0432
      \u0438\u0441\u0441\u043B\u0435\u0434\u043E\u0432\u0430\u0442\u044C \u043D\u0435\u0438\u0437\u0432\u0435\u0434\u0430\u043D\u043D\u044B\u0435
      \u0437\u0435\u043C\u043B\u0438 \u0438 \u043F\u0440\u043E\u0441\u043B\u0430\u0432\u0438\u0442\u044C
      \u043D\u0430\u0448\u0435 \u0432\u0435\u043B\u0438\u043A\u043E\u0435 \u0433\u043E\u0441\u0443\u0434\u0430\u0440\u0441\u0442\u0432\u043E."
  - speaker: "\u041F\u0435\u0442\u0440 \u041F\u0435\u0440\u0432\u044B\u0439"
    speech: "\u041E\u0442\u043B\u0438\u0447\u043D\u043E. \u0422\u044B \u043E\u0442\u043F\u0440\u0430\u0432\u0438\u0448\u044C\u0441\u044F
      \u043D\u0430 \"\u041D\u0435\u0432\u0430\". \u042D\u0442\u043E\u0442 \u043A\u043E\u0440\u0430\u0431\u043B\u044C
      \u043F\u043E\u0432\u0435\u0434\u0435\u0442 \u0442\u0435\u0431\u044F \u043A
      \u043D\u043E\u0432\u044B\u043C \u0433\u043E\u0440\u0438\u0437\u043E\u043D\u0442\u0430\u043C.
      \u041F\u043E\u043C\u043D\u0438, \u0447\u0442\u043E \u0442\u044B \u043D\u0435
      \u0442\u043E\u043B\u044C\u043A\u043E \u043C\u043E\u0440\u044F\u043A, \u043D\u043E
      \u0438 \u043F\u043E\u0441\u043B\u0430\u043D\u043D\u0438\u043A \u0420\u043E\u0441\u0441\u0438\u0438.
      \u0422\u0432\u043E\u0438 \u0434\u0435\u0439\u0441\u0442\u0432\u0438\u044F \u043E\u0442\u0440\u0430\u0437\u044F\u0442\u0441\u044F
      \u043D\u0430 \u0438\u043C\u0435\u043D\u0438 \u043D\u0430\u0448\u0435\u0439
      \u043D\u0430\u0446\u0438\u0438."
  - speaker: "\u041B\u0438\u0441\u044F\u043D\u0441\u043A\u0438\u0439"
    speech: "\u042F \u043D\u0435 \u043F\u043E\u0434\u0432\u0435\u0434\u0443 \u0412\u0430\u0441,
      \u0412\u0430\u0448\u0435 \u0412\u0435\u043B\u0438\u0447\u0435\u0441\u0442\u0432\u043E."
  - speaker: "\u041F\u0435\u0442\u0440 \u041F\u0435\u0440\u0432\u044B\u0439"
    speech: "\u0412\u043F\u0435\u0440\u0435\u0434, \u043A \u043D\u043E\u0432\u044B\u043C
      \u043E\u0442\u043A\u0440\u044B\u0442\u0438\u044F\u043C! \u0418 \u0434\u0430
      \u043F\u0440\u0435\u0431\u0443\u0434\u0435\u0442 \u0441 \u0442\u043E\u0431\u043E\u0439
      \u0443\u0434\u0430\u0447\u0430."
